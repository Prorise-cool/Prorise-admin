{
  "name": "prorise-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --project unit",
    "test:storybook": "vitest --project=storybook",
    "test:coverage": "vitest --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build:icons": "svgr ./src/assets/icons --out-dir ./src/components/icons --config-file ./.svgrrc.js"
  },
  "prepare": "lefthook install",
  "dependencies": {
    "@iconify/react": "^6.0.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@storybook/test": "^8.6.14",
    "@types/color": "^4.2.0",
    "antd": "^5.27.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color": "^5.0.2",
    "lucide-react": "^0.548.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@biomejs/biome": "^2.2.6",
    "@chromatic-com/storybook": "4.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@mdx-js/react": "^3.1.1",
    "@storybook/addon-a11y": "9.1.15",
    "@storybook/addon-docs": "9.1.15",
    "@storybook/addon-onboarding": "9.1.15",
    "@storybook/addon-themes": "^9.1.15",
    "@storybook/addon-vitest": "9.1.15",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react-vite": "9.1.15",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/vite": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser-playwright": "^4.0.3",
    "@vitest/coverage-v8": "^4.0.3",
    "@vitest/ui": "^4.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "9.1.15",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "lefthook": "^1.13.6",
    "markdown-to-jsx": "^8.0.0",
    "playwright": "^1.56.1",
    "storybook": "9.1.15",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-auto-import-antd": "^0.0.2",
    "vite": "npm:rolldown-vite@7.1.14",
    "vite-tsconfig-paths": "^5.1.4",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.3"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    }
  }
}
